{
  "name": "commiter",
  "version": "1.0.0",
  "description": "VS Code extension and CLI tool for generating git commit messages and committing changes via the git CLI.",
  "main": "dist/extension.js",
  "bin": {
    "commiter": "dist/cli.js"
  },
  "scripts": {
    "compile": "tsc -p .",
    "build": "npm run compile",
    "watch": "tsc -w -p .",
    "package": "vsce package"
  },
  "publisher": "zakriyaalisabir",
  "author": "Zakriya Ali Sabir",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onView:commiterView"
  ],
  "contributes": {
    "commands": [
      {
        "command": "commiter.generateCommit",
        "title": "Generate Commit",
        "icon": "$(git-commit)"
      },
      {
        "command": "commiter.showConfig",
        "title": "Show Config",
        "icon": "$(gear)"
      },
      {
        "command": "commiter.configure",
        "title": "Configure",
        "icon": "$(settings)"
      }
    ],
    "views": {
      "commiter": [
        {
          "id": "commiterView",
          "name": "Commiter",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "commiter",
          "title": "Commiter",
          "icon": "$(git-commit)"
        }
      ]
    }
  },
  "dependencies": {
    "@types/node": "^18.0.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "typescript": "^5.2.2",
    "@vscode/vsce": "^2.0.0"
  },
  "keywords": [],
  "license": "ISC"
}