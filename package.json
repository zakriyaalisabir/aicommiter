{
  "name": "commiter",
  "version": "1.0.0",
  "description": "VS Code extension and CLI tool for generating git commit messages and committing changes via the git CLI.",
  "main": "dist/extension.js",
  "bin": {
    "commiter": "dist/cli.js"
  },
  "scripts": {
    "compile": "tsc -p .",
    "build": "npm run compile",
    "watch": "tsc -w -p .",
    "package": "vsce package"
  },

  "publisher": "Zakriya Ali Sabir",
  "author": "Zakriya Ali Sabir",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onCommand:commiter.generateCommit",
    "onCommand:commiter.showConfig",
    "onCommand:commiter.configure"
  ],
  "contributes": {
    "commands": [
      {
        "command": "commiter.generateCommit",
        "title": "Commiter: Generate Commit"
      },
      {
        "command": "commiter.showConfig",
        "title": "Commiter: Show Config"
      },
      {
        "command": "commiter.configure",
        "title": "Commiter: Configure"
      }
    ]
  },
  "dependencies": {
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.50.0",
    "typescript": "^5.2.2",
    "@vscode/vsce": "^2.0.0"
  },
  "keywords": [],
  "license": "ISC"
}